{
  "name": "@hitechline/pdf-maker",
  "version": "1.0.0-canary.3",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "fix": "yarn lint:fix && yarn prettier:fix",
    "release": "semantic-release",
    "prepack": "yarn build",
    "prepublishOnly": "yarn build",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watchAll",
    "clean": "rimraf .tarball coverage dist typings",
    "clean:build": "rimraf dist typings",
    "lint": "eslint ./ --ext js,jsx,ts",
    "lint:fix": "eslint ./ --ext js,jsx,ts --fix",
    "prettier": "prettier --check ./**/*.{js,jsx,ts}",
    "prettier:fix": "prettier --write ./**/*.{js,jsx,ts}",
    "build": "run-s build:{check,types,cjs,esm}",
    "prebuild": "yarn clean:build",
    "build:check": "tsc --noEmit",
    "build:types": "tsc -p build/tsconfig.types.json",
    "build:cjs": "node build/cjs",
    "build:esm": "node build/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitechline/pdf-maker.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "react",
    "pdf",
    "maker",
    "handlebars",
    "puppeteer"
  ],
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "handlebars": "^4.7.7"
  },
  "peerDependenciesMeta": {
    "handlebars": {
      "optional": true
    }
  },
  "dependencies": {
    "puppeteer": "^20.0.0"
  },
  "devDependencies": {
    "@hitechline/eslint-config": "2.0.2",
    "@hitechline/semantic-release-config": "1.2.0",
    "@jest/globals": "^29.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.3",
    "@semantic-release/npm": "10.0.4",
    "@semantic-release/release-notes-generator": "11.0.3",
    "@types/jest": "^29.0.0",
    "@types/jest-image-snapshot": "^6.0.0",
    "@types/node": "16.9.1",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "conventional-changelog-angular": "6.0.0",
    "conventional-changelog-conventionalcommits": "6.1.0",
    "esbuild": "0.18.10",
    "eslint": "8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "handlebars": "^4.7.7",
    "jest": "^29.0.0",
    "jest-image-snapshot": "^6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "semantic-release": "17.4.7",
    "ts-jest": "^29.0.0",
    "typescript": "5.1.6"
  }
}
